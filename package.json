{
  "name": "react-workspace",
  "version": "1.0.0",
  "description": "Some basic React stuff",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack --mode development && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline",
    "build:prod": "webpack --mode production && cp src/index.html dist/index.html"
  },
  "keywords": [
    "reactjs"
  ],
  "author": "Paul Sczurek",
  "license": "MIT",
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4"
  }
}
